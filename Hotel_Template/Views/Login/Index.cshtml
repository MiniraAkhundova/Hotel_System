@{
    ViewData["Title"] = "LogIn Page";

}
@model project.DTO.AdminstratorsDTOs.AdminToListDTO

<link rel="icon" type="image/png" href="~/css/images/icons/favicon.ico" />

<link href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet">

<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="~/css/login_util.css">
<link rel="stylesheet" type="text/css" href="~/css/login_main.css">
<!--===============================================================================================-->

<div class="limiter">
    <div class="container-login100" >
        <div class="wrap-login100">
            <form method="post" asp-controller="Pages" asp-action="AdminExistenseChecking" class="login100-form validate-form">
                <span class="login100-form-logo">
                    <img src="~/login_images/icons/logo_light.svg" />
                </span>

                <span class="login100-form-title p-b-34 p-t-27">
                    Log in
                </span>

                <div class="wrap-input100 validate-input" data-validate="Enter username">
                    <input asp-for="AdminName" class="input100" type="text" placeholder="User Name">

                    <img src="~/login_images/icons/user (3).png" />

                </div>

                <div class="wrap-input100 validate-input" data-validate="Enter password">
                    <input asp-for="AdminPassword" class="input100" type="password" placeholder="Password">
                    <img src="~/login_images/icons/padlock.png" />
                </div>


                <div class="contact100-form-checkbox">

                    <input style=" width: 16px; height: 16px;" type="checkbox">
                    <label class="label-checkbox100" for="ckb1">
                        I Agree to the Terms and Contidions
                    </label>
                </div>

                <div class="container-login100-form-btn">

                    <button type="submit" class="login100-form-btn">
                        LogIn
                    </button>
                </div>

                <div class="text-center p-t-90">
                    <a class="txt1" href="#" style=" font-size: 16px !important;">
                        Don't have an Account ? <a asp-area="" asp-controller="Registration" asp-action="Index" style="margin-left: 10px; color:#B42828;font-size:18px;text-decoration:none">Register</a>
                    </a>
                </div>
            </form>
        </div>
    </div>
</div>
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>



<div id="dropDownSelect1"></div>


